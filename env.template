# 选择使用的模型: tongyi 或 deepseek
AI_MODEL_TYPE=deepseek

# 通义模型配置
TONGYI_API_KEY=

# Deepseek API 配置
DEEPSEEK_API_KEY=

# 开发环境配置
NODE_TLS_REJECT_UNAUTHORIZED=0

# MySQL 配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password
MYSQL_DATABASE=ai_writer

# PostgreSQL 配置
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DATABASE=ai_writer